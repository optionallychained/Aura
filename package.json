{
    "name": "protogl",
    "version": "0.5.0",
    "author": "jonnopon",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git://git@github.com/jonnopon/ProtoGL.git"
    },
    "main": "publish/index.js",
    "types": "publish/index.d.ts",
    "files": [
        "publish/"
    ],
    "scripts": {
        "lint": "eslint --ext .ts --fix src",
        "test": "jest",
        "docs:serve": "browser-sync start --port 9090 --server './docs' --files './docs/**/*'",
        "docs:watch": "nodemon -w ./src/docs --ext pug,ts,scss,md,png,jpg,svg ./scripts/docgen.js --args dev",
        "docs:dev": "concurrently \"npm run docs:serve\" \"npm run docs:watch\"",
        "docs:dist": "node ./scripts/docgen.js",
        "demo:debug": "webpack serve --config webpack.demo.config.js --mode development",
        "demo:dev": "webpack serve --config webpack.demo.config.js --mode development --open 'Chrome'",
        "predemo:dist": "npm run lint",
        "demo:dist": "webpack --config webpack.demo.config.js --mode production",
        "preengine:dist": "npm run lint",
        "engine:dist": "webpack --config webpack.engine.config.js",
        "engine:publish": "tsc --project ./tsconfig.publish.json",
        "prepublishOnly": "npm run engine:dist && npm run engine:publish"
    },
    "devDependencies": {
        "@types/jest": "^26.0.20",
        "@typescript-eslint/eslint-plugin": "^4.15.1",
        "@typescript-eslint/parser": "^4.15.1",
        "autoprefixer": "^10.2.4",
        "browser-sync": "^2.26.14",
        "concurrently": "^6.0.0",
        "copy-webpack-plugin": "^7.0.0",
        "css-loader": "^5.0.2",
        "eslint": "^7.20.0",
        "highlight.js": "^10.6.0",
        "html-webpack-plugin": "^5.2.0",
        "jest": "^26.6.3",
        "marked": "^2.0.0",
        "mini-css-extract-plugin": "^1.3.8",
        "nodemon": "^2.0.7",
        "normalize.css": "^8.0.1",
        "postcss-loader": "^5.0.0",
        "pug": "^3.0.0",
        "sass": "^1.32.8",
        "sass-loader": "^11.0.1",
        "ts-jest": "^26.5.1",
        "ts-loader": "^8.0.17",
        "typedoc": "^0.20.27",
        "typescript": "^4.1.5",
        "webpack": "^5.23.0",
        "webpack-cli": "^4.5.0",
        "webpack-dev-server": "^3.11.2"
    }
}
