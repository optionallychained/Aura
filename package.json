{
    "name": "protogl",
    "version": "0.0.1",
    "author": "jonnopon",
    "repository": {
        "type": "git",
        "url": "git://git@github.com/jonnopon/ProtoGL-TS.git"
    },
    "main": "publish/protogl.js",
    "types": "publish/protogl.d.ts",
    "files": [
        "publish/"
    ],
    "scripts": {
        "lint": "eslint --ext .ts --fix",
        "demo:debug": "webpack serve --config webpack.demo.config.js",
        "demo:dev": "webpack serve --config webpack.demo.config.js --open 'Chrome'",
        "predemo:dist": "npm run lint",
        "demo:dist": "webpack --config webpack.demo.dist.config.js",
        "engine:dist": "webpack --config webpack.engine.dist.config.js",
        "prepublishOnly": "npm run lint && npm run engine:dist && tsc --project ./tsconfig.publish.json"
    },
    "devDependencies": {
        "@typescript-eslint/eslint-plugin": "^4.10.0",
        "@typescript-eslint/parser": "^4.10.0",
        "eslint": "^7.16.0",
        "html-webpack-plugin": "^4.5.0",
        "ts-loader": "^8.0.12",
        "typescript": "^4.1.3",
        "webpack": "^5.11.0",
        "webpack-cli": "^4.2.0",
        "webpack-dev-server": "^3.11.0"
    },
    "dependencies": {}
}
