{
    "name": "protogl",
    "version": "0.0.6",
    "author": "jonnopon",
    "repository": {
        "type": "git",
        "url": "git://git@github.com/jonnopon/ProtoGL-TS.git"
    },
    "main": "publish/protogl.js",
    "types": "publish/protogl.d.ts",
    "files": [
        "publish/"
    ],
    "scripts": {
        "lint": "eslint --ext .ts --fix",
        "test": "jest",
        "docs:serve": "browser-sync start --port 9090 --server './docs' --files './docs/**/*'",
        "docs:watch": "nodemon -w ./src/docs --ext pug,ts,scss,md,png,jpg,svg ./scripts/docgen.js --args dev",
        "docs:dev": "concurrently \"npm run docs:serve\" \"npm run docs:watch\"",
        "docs:dist": "node ./scripts/docgen.js",
        "demo:debug": "webpack serve --config webpack.demo.config.js --mode development",
        "demo:dev": "webpack serve --config webpack.demo.config.js --mode development --open 'Chrome'",
        "predemo:dist": "npm run lint",
        "demo:dist": "webpack --config webpack.demo.config.js --mode production",
        "preengine:dist": "npm run lint",
        "engine:dist": "webpack --config webpack.engine.config.js",
        "engine:publish": "tsc --project ./tsconfig.publish.json",
        "prepublishOnly": "npm run engine:dist && npm run engine:publish"
    },
    "devDependencies": {
        "@types/jest": "^26.0.20",
        "@typescript-eslint/eslint-plugin": "^4.14.1",
        "@typescript-eslint/parser": "^4.14.1",
        "autoprefixer": "^10.2.3",
        "browser-sync": "^2.26.14",
        "concurrently": "^5.3.0",
        "css-loader": "^5.0.1",
        "eslint": "^7.18.0",
        "highlight.js": "^10.5.0",
        "html-webpack-plugin": "^4.5.1",
        "jest": "^26.6.3",
        "marked": "^1.2.8",
        "mini-css-extract-plugin": "^1.3.5",
        "nodemon": "^2.0.7",
        "normalize.css": "^8.0.1",
        "postcss-loader": "^4.2.0",
        "pug": "^3.0.0",
        "sass-loader": "^10.1.1",
        "ts-jest": "^26.4.4",
        "ts-loader": "^8.0.14",
        "typedoc": "^0.20.19",
        "typescript": "^4.1.3",
        "webpack": "^5.18.0",
        "webpack-cli": "^4.4.0",
        "webpack-dev-server": "^3.11.2"
    }
}
